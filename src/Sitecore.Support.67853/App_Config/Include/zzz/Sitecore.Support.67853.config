<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:x="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <pipelines>
      <wffm.executeSaveActions>
        <processor
          type="Sitecore.Support.Forms.Mvc.Pipelines.ExecuteSaveActions.SaveDataAndExecuteSaveActions,Sitecore.Support.67853"
          patch:instead="*[@type='Sitecore.Forms.Mvc.Pipelines.ExecuteSaveActions.SaveDataAndExecuteSaveActions,Sitecore.Forms.Mvc']">
          <param name="actionExecutor" ref="/sitecore/wffm/wffmActionExecutor" />
          <param name="analyticsTracker" ref="/sitecore/wffm/analytics/analyticsTracker" />
        </processor>
      </wffm.executeSaveActions>
    </pipelines>
    <settings>
      <!-- POSTED FILES LIMIT
			   Sets max size of posted files  -->
      <setting name="WFM.PostedFilesLimit" value="1024KB" patch:after="*[@name='WFM.InitialValuesCacheSize']" />
    </settings>
  </sitecore>
</configuration>